    <!-- Navbar Start -->
    <nav class="navbar navbar-expand-lg bg-white navbar-light shadow sticky-top p-0">
      <a href="/" class="navbar-brand d-flex align-items-center px-4 px-lg-5">
          <h2 class="m-0 text-primary"><i class="fa fa-book me-3"></i>Wato</h2>
      </a>
      <button type="button" class="navbar-toggler me-4" data-bs-toggle="collapse" data-bs-target="#navbarCollapse">
          <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarCollapse">
          <div class="navbar-nav ms-auto p-4 p-lg-0">
              <a href="/admin/courses" class="nav-item nav-link active">Courses</a>
              <a href="/about" class="nav-item nav-link">About</a>
              <a href="/contact" class="nav-item nav-link">Contact</a>
              <a href="/resources" class="nav-item nav-link">Resources</a>
              <a href="/faqs" class="nav-item nav-link">FAQs</a>
              <a href="/students" class="nav-item nav-link">Student Portal</a>
          </div>
          <% if(!currentUser) {%>
              <a class="nav-item nav-link active" href="/user/login">Login</a>
            <a href="/user/register" class="btn btn-primary py-4 px-lg-5 d-none d-lg-block">Join Now<i class="fa fa-arrow-right ms-3"></i></a>
          <% } else {%>
            <div class="nav-item dropdown">
              <button type="button" class="btn btn-info">Welcome to Wato, <%= currentUser.firstname %></button>
              <div class="dropdown-menu fade-down m-0">
                  <a href="/profile" class="dropdown-item">Profile</a>
                  <a href="/account-setting" class="dropdown-item">Setting</a>
                  <a href="/purchase" class="dropdown-item">Purchase</a>
                  <a href="/user/logout" class="dropdown-item text-danger">Logout</a>
              </div>
            </div>
            
          <% } %>
      </div>
    </nav>
    <!-- Navbar End -->




